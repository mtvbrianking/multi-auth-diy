{
    "name": "module/seller",
    "type": "library",
    "require": {
        "laravel/framework": "^10.0"
    },
    "require-dev": {},
    "autoload": {
        "psr-4": {
            "Seller\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Seller\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Seller\\SellerServiceProvider"
            ]
        }
    }
}
